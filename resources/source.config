Graphite.Key: "Rubicon"
HTTP.ContentType: "application/json"
Legacy.RTBId: 6
Decoder: "versatile"
Encoder: "versatile"
Macro.Price: "%%bid_price%%"
Encoder.Versatile.Dialect: "auction_id = requestid bid = if (slot[0].price, slot[0].price, 0) ad_tag = slot[0].invocationcode advertiser_name = slot[0].clickthroughurl creative_id = slot[0].creativecode deal = slot[0].dealid seat_ids[$k] = [ \"criteo\" ]"
Decoder.Versatile.Dialect: "proposal = {     analysis = 1 # Decoded #     publisher =     {         id = site_id         url = page_url     }     exclusions=     {         keys =         [             concat(\"Rubicon\", \"|\", publisher_id, if(site_id, concat(\"|\", site_id)))         ]     }     slots =     [         {             exclusions =             {                 rules[$type] =                 {                     level = 2 # client #                     lookup = \"rtb.rubicon.sensitive\"                     value = denied_ad_types[$type]                 }             }             deals[$deal] =             {                 floor = pmp.deals[$deal].floor                 id = pmp.deals[$deal].id             }             intention = 0 # Accept #             sizes =             [                 {                     width = ad_width_px                     height = ad_height_px                 }             ]             visibility = select(page_position, \"above-the-fold\", 1, \"below-the-fold\", 0, nan())         }     ]     request =     {         id = auction_id     }     user =     {         country = slice(country, 0, 2)         criteoid = network_user_id         encryptionid = 2         ipaddress = ip_address         language = accept_language         useragent = user_agent     }     features =     {         carrier = device.carrier         vertical = site_cat[0]     } }"
